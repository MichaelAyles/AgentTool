{
  "name": "@vibecode/frontend",
  "version": "1.0.0",
  "description": "Frontend React application for Vibe Code",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:vercel": "NODE_ENV=production vite build --mode production --logLevel warn",
    "build:fast": "vite build --mode production --logLevel error",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "test": "vitest",
    "vercel-build": "vite build --mode production",
    "build:static": "vite build --mode production --logLevel error",
    "vercel-dev": "npm run dev",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.14.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.3.6",
    "uuid": "^11.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-search": "^0.13.0",
    "xterm-addon-web-links": "^0.9.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "typescript": "^5.3.0",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  }
}
