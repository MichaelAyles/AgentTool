# Local development environment variables
# Copy this to .env.local and customize as needed

VITE_APP_NAME=Vibe Code (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_API_BASE_URL=http://localhost:3000/api
VITE_DESKTOP_CONNECTOR_URL=http://localhost:3000
VITE_BACKEND_TYPE=auto

# Development flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEBUG=true

# Feature flags
VITE_FEATURE_DESKTOP_CONNECTOR=true
VITE_FEATURE_CLOUD_BACKEND=true
VITE_FEATURE_MULTI_BACKEND=true

# External URLs
VITE_DOCS_URL=http://localhost:3001/docs
VITE_GITHUB_URL=https://github.com/your-org/vibe-code
VITE_SUPPORT_URL=mailto:support@vibecode.com